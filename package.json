    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "node-sass": "^9.0.0",
    "parcel": "^2.10.0",
    "prettier": "3.0.3",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.69.3",
    "sass-lint": "~1.13.1",
    "sass-mq": "~6.0.0",
    "uglify-js": "^3.17.4"
  },
